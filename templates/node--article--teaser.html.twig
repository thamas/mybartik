{% extends "node--article.html.twig" %}
{#
/**
 * @file
 * Theme override to display an article teaser.
 */
#}

{% block node %}
  <article{{ attributes.addClass(classes, 'clearfix' ) }}>

    {{ content.field_image }}

    {% block node_submitted %}
      {{ parent() }}
    {% endblock %}

    {% block node_title %}
      {{ parent() }}
    {% endblock %}

    {% block node_content %}
      {{ parent() }}
    {% endblock %}

  </article>
{% endblock %}
